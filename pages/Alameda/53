    function OpenDetail(theID) {
        thePage = "prosoma.dll?w6=301329&searchby=offender&id=" + theID

        window.open(thePage, 'OffenderDetail', 'width=700,height=520,resizable=yes,scrollbars=yes');
    }
    var theNewsWin = null;

    function newsWin(theNewsUrl) {

        if (theNewsWin != null) {
            // The following line may be used to test that the variable is being set correctly.
            //alert("Closed Window!");
            theNewsWin.close();
        }


        var theNewsWin = window.open(theNewsUrl, "DetailInfo", "scrollbars=yes,resizable=yes,HEIGHT=350,WIDTH=200,status=no,toolbar=yes,directories=no,menubar=no,location=no");
    }

